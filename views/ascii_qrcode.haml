%html
  %head
    %title Input ASCII-armored PGP Key
    %link{:rel => "stylesheet", :type => "text/css", :href => "/css/ascii.css"}
  %body
    %table
      - @qr.modules.each_index do |x|
        %tr
          - @qr.modules.each_index do |y|
            - if @qr.dark?(x,y)
              %td.black
            - else
              %td.white
